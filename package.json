{
  "name": "gulp-grapher-core",
  "version": "0.0.1",
  "description": "core logic for a gulp graph interface Edit",
  "keywords": [
    "gulp",
    "gulpjs",
    "graph",
    "graphlib"
  ],
  "author": "kmannislands",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "prepublish": "node_modules/babel-cli/bin/babel.js src --plugins remove-comments --out-dir lib"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add",
      "node_modules/babel-cli/bin/babel.js src --plugins remove-comments --out-dir lib"
    ]
  },
  "dependencies": {
    "acorn": "^5.1.1",
    "babel-plugin-remove-comments": "^2.0.0",
    "graphlib": "^2.1.1",
    "graphlib-dot": "^0.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "esdoc": "^0.5.2",
    "esdoc-node": "^1.0.2",
    "eslint": "^4.2.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-prettier": "^2.1.2",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.1",
    "prettier": "^1.5.3"
  }
}
